<div class ="Brownies">
<h3>Here are my teeny tiny URLs</h3>

<% for (const i in urls) {%>
  <p> <%= i %> -- <%= urls[i] %></p>

  <form method="GET" action="/urls/<%= i %>/edit">
  <input type="submit" value="Edit"></form>

  <form method="POST" action="/urls/<%= i %>/delete">
  <input type="submit" value="Delete"></form>
<% } %>

</div>
<div class = "new URL">
  <br />
<a href = "http://localhost:8080/urls/new"> Create New URL </a>
</div>
